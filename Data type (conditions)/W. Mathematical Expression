#include<stdio.h>
int main()
{
    long long a,b,c;
    char x,y;
    scanf("%lld %c %lld %c %lld", &a, &x, &b, &y, &c);
    if((x=='+' && a+b==c) || (x=='-' && a-b==c) || (x=='*' && a*b==c))
    {
        printf("Yes");
    }
    else if(x=='+')
    {
        printf("%d", a+b);
    }
    else if(x=='-')
    {
        printf("%d", a-b);
    }
    else if(x=='*')
    {
        printf("%d", a*b);
    }
    return 0;
}